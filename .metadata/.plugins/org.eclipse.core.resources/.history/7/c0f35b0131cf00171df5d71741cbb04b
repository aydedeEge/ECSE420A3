package ca.mcgill.ecse420.a3;

public class QNode {
	public String fruit;
	public int key;
	public QNode next;
	
	private QNode(QNodeBuilder qbb){
		this.fruit = qbb.fruit;
		this.key = fruit.hashCode();
		this.next = qbb.next;
	}
	
	public class QNodeBuilder {
		private String fruit;
		private int key;
		private QNode next;
		
		public QNodeBuilder(String fruit){
			this.fruit = fruit;
			this.key = this.fruit.hashCode();
		}
		
		public QNodeBuilder setNext(QNode next){
			this.next = next;
			return this;
		}
		
		public QNode build(){
			return new QNode(this);
		}
	}
	
	
}
